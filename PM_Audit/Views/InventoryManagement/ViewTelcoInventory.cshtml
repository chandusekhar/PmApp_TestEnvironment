        function ClosePMSuccessfully() {
            debugger;
            var objPMClose = new Object();
            objPMClose.PM_ID = PM_ID;
            objPMClose.SiteCode = SiteCode;
            objPMClose.PM_Type = 'Telco';
            ApiCallService.Ajax('@System.Configuration.ConfigurationManager.AppSettings["ApiPath"]', "api/PreventiveMaintenance/ClosePM", objPMClose, onSuccessClosePM, "POST")
        }
        function onSuccessClosePM(results) {
            if (results == "Success") {
                alertify.alert('Success', 'PM has been closed successfully!')
                window.location.href = "/PreventiveMaintenance/ViewPM";
            }
            else {
                alertify.alert('Failure', results);
            }
        }
        function GetCellInfo() {
            debugger;
            if (SiteCode) {
                var data = new Object();
                data.SiteCode = SiteCode;
                data.Type = $('#dd_Serving_Technology').val();
                ApiCallService.Ajax('@System.Configuration.ConfigurationManager.AppSettings["ApiPath"]', "api/UtilityController/GetSiteCellsInformation", data, OnSuccessGetCellInfo)
            }
        }

        function OnSuccessGetCellInfo(result) {
            debugger;
            $('#cell_id_bts_cabinet').text('');
            $('#cell_name_bts_cabinet').text('');
           // $('select#cell_id_bts_cabinet').append('<option>Select</option>');
            $('select#cell_name_bts_cabinet').append('<option value="NA" id="select_option_name_bts_cabinet" selected disabled hidden>Select...</option>');
            for (var counter = 0; counter < result.length; counter++) {
               // $('select#cell_id_bts_cabinet').append('<option>' + result[counter].CellName + '</option>');
                $('select#cell_name_bts_cabinet').append('<option>' + result[counter].CellName + '</option>');
            }
            $('select#cell_name_bts_cabinet').append('<option>NA</option>')

        }
